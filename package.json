{
    "private": true,
    "name": "myName",
    "engines": {
        "node": "10.14.2"
    },
    "scripts": {
        "client": "cd packages/client && yarn start",
        "server": "cd packages/server && yarn start",
        "start": "yarn install && concurrently --kill-others-on-fail \"yarn server\"  \"yarn client\""
    },
    "workspaces": [
        "packages/*"
    ],
    "dependencies": {
        "concurrently": "4.1.0"
    }
}
